<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated safetycase -->
      <package
            uri="http://se.cs.toronto.edu/modelepedia/SafetyCase"
            class="edu.toronto.cs.se.modelepedia.safetycase.SafetyCasePackage"
            genModel="model/safetycase.genmodel"/>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.models">
      <modelType>
         <type
               name="SafetyCase"
               uri="http://se.cs.toronto.edu/modelepedia/SafetyCase">
         </type>
      </modelType>
   </extension>

   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            class="edu.toronto.cs.se.mmint.mid.relationship.RelationshipPackage"
            uri="http://se.cs.toronto.edu/modelepedia/SCRel">
      </package>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.relationships">
      <relationshipType>
         <modelType>
            <type
                  name="SCRel"
                  uri="http://se.cs.toronto.edu/modelepedia/SCRel">
            </type>
         </modelType>
         <modelTypeEndpoint>
            <type
                  name="SCEndpoint"
                  uri="http://se.cs.toronto.edu/modelepedia/SCRel-SafetyCase/SCEndpoint">
            </type>
            <typeEndpoint
                  lowerBound="0"
                  upperBound="-1"
                  targetTypeUri="http://se.cs.toronto.edu/modelepedia/SafetyCase">
            </typeEndpoint>
         </modelTypeEndpoint>
      </relationshipType>
   </extension>

</plugin>
